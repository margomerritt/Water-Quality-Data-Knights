-- Database: Beach_water_db

-- DROP DATABASE IF EXISTS "Beach_water_db";

CREATE DATABASE "Beach_water_db"
    WITH
    OWNER = postgres
    ENCODING = 'UTF8'
    LC_COLLATE = 'C'
    LC_CTYPE = 'C'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1
    IS_TEMPLATE = False;

COMMENT ON DATABASE "Beach_water_db"
    IS 'This is our starting database for our final project.';
